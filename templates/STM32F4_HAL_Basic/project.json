{
    "name": "STM32F4_HAL_Basic",
    "version": "1.0.0",
    "description": "Basic STM32F4 HAL project with GPIO LED blink",
    "target": {
        "mcu": "STM32F407VGT6",
        "family": "STM32F4",
        "clock": 168000000,
        "flash": 1048576,
        "ram": 196608
    },
    "build": {
        "optimization": "-O2",
        "defines": [
            "USE_HAL_DRIVER",
            "STM32F407xx"
        ],
        "cflags": [
            "-Wall",
            "-g"
        ],
        "includes": [
            "Inc",
            "Drivers/STM32F4xx_HAL_Driver/Inc"
        ]
    },
    "linker": {
        "script": "STM32F407VGTx_FLASH.ld",
        "libraries": [
            "-lc",
            "-lm",
            "-lnosys"
        ],
        "ldflags": [
            "-Wl,--gc-sections"
        ]
    }
}